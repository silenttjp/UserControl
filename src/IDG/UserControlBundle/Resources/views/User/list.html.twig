

<div class="panel panel-default">
  <!-- Default panel contents -->
  <div class="panel-heading">Current Users</div>


  <!-- List group -->
  <ul class="list-group">
      {% for user in users %}
    <li class="list-group-item">{{ user.FirstName }} {{ user.LastName }}
        <button id="user-{{ user.id }}" class="btn btn-danger btn-xs glyphicon glyphicon-remove pull-right"></button>
    </li>
    {% endfor %}
  
  </ul>
</div>
